			android:layout_alignEnd="@id/item2_icon"
			android:layout_alignStart="@id/item2_icon"
			android:text="@string/uninstall"
			android:id="@+id/item2_name"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content" />
	</RelativeLayout>
    <RelativeLayout
		android:background="@drawable/press_border_bg_up_none"
		android:id="@+id/item3"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content">
