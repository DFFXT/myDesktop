<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
	android:orientation="horizontal"
	android:background="@color/transparentWhite"
	android:padding="@dimen/longClickItemPadding"
    >
	<RelativeLayout
		android:id="@+id/item1"
		android:background="@drawable/press_border_bg_up_none"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content">
		<ImageView
			android:src="@drawable/hidden"
			android:padding="@dimen/longClickItemPadding"
			android:id="@+id/item1_icon"
			android:layout_width="@dimen/longClickItemSize"
			android:layout_height="@dimen/longClickItemSize"
			android:contentDescription="@string/action_settings"
			/>
		<TextView
			android:textSize="@dimen/textSize_min"
			android:gravity="center"
			android:layout_below="@id/item1_icon"
			android:layout_alignEnd="@id/item1_icon"
			android:layout_alignStart="@id/item1_icon"
			android:text="@string/longClickName_item1"
			android:id="@+id/item1_name"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content" />
	</RelativeLayout>
	<RelativeLayout
		android:background="@drawable/press_border_bg_up_none"
		android:id="@+id/item2"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content">
