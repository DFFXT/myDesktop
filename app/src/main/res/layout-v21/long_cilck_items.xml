		<TextView
			android:textSize="@dimen/textSize_min"
			android:gravity="center"
			android:layout_below="@id/item6_icon"
			android:layout_alignEnd="@id/item6_icon"
			android:layout_alignStart="@id/item6_icon"
			android:text="@string/longClickName_item6"
			android:id="@+id/item6_name"
