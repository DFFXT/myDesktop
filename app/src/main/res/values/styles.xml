<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="Dialog">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@color/_transparentWhite</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>
    <style name="AppBaseTheme" parent="android:Theme.Light.NoTitleBar">

    </style>
    <style name="WhiteL" parent="@android:style/Theme.Holo.Dialog">
        <item name="android:background">@android:color/white</item>
    </style>
    <style name="T" parent="@android:style/Theme.Holo.Dialog">
        <item name="android:background">@android:color/holo_purple</item>
    </style>

</resources>
